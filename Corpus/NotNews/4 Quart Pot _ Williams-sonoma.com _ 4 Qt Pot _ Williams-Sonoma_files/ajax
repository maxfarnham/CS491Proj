	       	            { selector: "ws", paramName: "chkWestElm" },
		            { selector: "pb", paramName: "chkPotteryBarn" },
	       	            { selector: "pk", paramName: "chkPotteryBarnKids" },
		            { selector: "mg", paramName: "chkMarkAndGraham" },
	       	            { selector: "catalogSignUpCheck", paramName: "catSignUp" }
	       	   ]);   
	       	   params.splice(0,0,window.profileMboxName,"clicked=submit2")
	       	   mboxUpdate.apply({},params);
	       	});
	    } else {
                setTimeout("tntLookForSubmit2()",500);
            }
        } // function tntLookForSubmit2

	window.adEmailSetSessionCookie = function()
	{
		if( document.cookie.indexOf('ad_sess_wsi_email') < 0){
			
			var thisDate = new Date();
			thisDate.setDate(thisDate.getDate() + 14);
			mboxUpdate(window.profileMboxName, "profile.nextWSIEmailPop=" + thisDate + "");
			document.cookie = "ad_sess_wsi_email=true; expires=" + thisDate + "; path=/";
			return false;
		}else{
			return true;
		}
	}

	window.adSwizEmailBG = function(){
		var adCtr = 0;
		var d = document.getElementsByTagName('div');
		for(var i=0;i<d.length;i++){
			if(d[i].id && d[i].id == "adEmailPopupBG"){
				adCtr++;
			}
		}
		for(var i=0;i<d.length;i++){
			if(d[i].className && d[i].className == 'dijitDialogUnderlayWrapper'){ d[i].parentNode.removeChild(d[i]); i--; }
		}
		if(adCtr == 1){
			var p = document.getElementById('adEmailPopupBG');
			p.style.display="block";
			document.getElementById('adEmailPopupBGVis').style.width = (window.getPageSize().pageWidth) + 'px';
			document.getElementById('adEmailPopupBGVis').style.height = (window.getPageSize().pageHeight) + 'px';
			document.body.insertBefore(p, document.body.firstChild)
		}
	}

	window.adSwizEmailPop = function(){
		var adCtr = 0;
		var d = document.getElementsByTagName('div');
		for(var i=0;i<d.length;i++){
			if(d[i].id && d[i].id == "adEmailPopup"){
				adCtr++;
			}
		}
		if(adCtr == 1){
			var p = document.getElementById('adEmailPopup');
				p.style.display="block";
			document.body.insertBefore(p, document.body.firstChild)
		}
	}

	window.adEmailPopup = function()
	{
		var p;
		var pbg;
	
		var d = document.getElementsByTagName('div');
		for(var i=0;i<d.length;i++){
	
			if(d[i].id && d[i].id == "adEmailPopup"){
				p = d[i];
				d[i].style.display = "block";
			}
			if(d[i].id && d[i].id == "adEmailPopupBG"){
				pbg = d[i];
				d[i].style.display = "block";
			}
		}
	
		if(p && pbg){ 
			p.style.display = "block"; 
			pbg.style.display = "block"; 
			document.getElementById('adEmailPopupBGVis').style.width = (window.getPageSize().pageWidth) + 'px';
			document.getElementById('adEmailPopupBGVis').style.height = (window.getPageSize().pageHeight) + 'px';
		}
	}

	window.adEmailClosePopup = function()
	{
		var p;
		var pbg;

		var d = document.getElementsByTagName('div');
		for(var i=0;i<d.length;i++){

			if(d[i].id && d[i].id == "adEmailPopup"){
				p = d[i];
				d[i].style.display = "none";
			}
			if(d[i].id && d[i].id == "adEmailPopupBG"){
				pbg = d[i];
				d[i].style.display = "none";
			}
		}
		if(p && pbg){ p.style.display = "none"; pbg.style.display = "none";}
		mboxUpdate(window.profileMboxName, 'clicked=closePopup');
	
	}

	window.getPageSize = function() {
		
		var isIE;
		if(navigator.appName.indexOf('Internet Explorer') > -1){
			isIE = true;
		}else{
			isIE = false;
		}
        var e = document,
        i = window,

        k = e.compatMode && e.compatMode != "BackCompat" ? e.documentElement: e.body;
        var g = e.body;
        var l = (i.innerWidth && i.scrollMaxX) ? i.innerWidth + i.scrollMaxX: Math.max(g.scrollWidth, g.offsetWidth),
        f = (i.innerHeight && window.scrollMaxY) ? i.innerHeight + i.scrollMaxY: Math.max(g.scrollHeight, g.offsetHeight),
        h = isIE ? k.scrollWidth: (e.documentElement.clientWidth || self.innerWidth),
        c = isIE ? Math.max(k.scrollHeight, k.clientHeight) : (e.documentElement.clientHeight || self.innerHeight);
        var a = isIE ? k.clientWidth: (e.documentElement.clientWidth || self.innerWidth),
        j = isIE ? k.clientHeight: self.innerHeight;
        return {
            pageWidth: Math.max(h, l),
            pageHeight: Math.max(c, f),
            width: a,
            height: j,
            scrollLeft: isIE ? k.scrollLeft: pageXOffset,
            scrollTop: isIE ? k.scrollTop: pageYOffset
        }
    }

	window.adEmailCheckTestParam = function()
	{
		if(location.search.indexOf("fid=") > -1){
			var s = location.search.split("&");
			for(var i=0;i<s.length;i++){
				if(s[i].indexOf("fid=") > -1){
					return s[i].split("=")[1];
				}
			}
		}
		return "";
	}

