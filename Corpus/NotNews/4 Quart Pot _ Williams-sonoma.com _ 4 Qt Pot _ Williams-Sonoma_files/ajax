	    this.lookupHandler = function(retObj) {        

	        var idVal = retObj; 
			if(idVal){
				var a = idVal.split("|");
				for(var i=0;i<a.length;i++){
					var b = a[i].split('=');
					if(b.length > 1){
						if(b[0] == "return_code"){
							adParams.splice(0, 0, "profile.return_code_emailList_" + window.profileSpecifier + "=" + b[1]);
						}else{
							adParams.splice(0, 0, "profile." + window.profileSpecifier + "_" + b[0] + "=" + b[1]);
						}
					}else if(a[i].indexOf('NO MATCH') > -1){
						adParams.splice(0, 0, "profile.return_code_emailList_" + window.profileSpecifier + "=NO MATCH");
						window.adDisplayEmailOverlay();
					}
				}
				adEmailSetSessionCookie();
				
				if(adParams.length > 0){
					adParams.splice(0, 0, "profile.the_id_reset_" + window.profileSpecifier +  "=true");
					adParams.splice(0, 0, window.profileMboxName);
					mboxUpdate.apply(this, adParams);
				}else{
					adParams.splice(0, 0, "profile.return_code_emailList_" + window.profileSpecifier + "=");
					adParams.splice(0, 0, window.profileMboxName);
					mboxUpdate.apply(this, adParams);
					window.adDisplayDefault();
				}
			}
	    }
	
		this.setEmailCodeHandler = function(retObj) {        
			adParams = new Array();
	        var idVal = retObj; 
			if(idVal){
				var a = idVal.split("|");
				for(var i=0;i<a.length;i++){
					var b = a[i].split('=');
					if(b.length > 1){
						if(b[0] == "email_return_code"){
							adParams.splice(0, 0, "profile.return_code_emailList_" + window.profileSpecifier + "=" + b[1]);
						}else{
							adParams.splice(0, 0, "profile." + window.profileSpecifier + "_" + b[0] + "=" + b[1]);
						}
					}else if(a[i].indexOf('NO MATCH') > -1){
						adParams.splice(0, 0, "profile.return_code_emailList_" + window.profileSpecifier + "=NO MATCH");
					}
				}

				if(adParams.length > 0){
					adParams.splice(0, 0, window.profileMboxName);
					mboxUpdate.apply(this, adParams);
				}else{
					adParams.splice(0, 0, window.profileMboxName);
					mboxUpdate.apply(this, adParams);
				}
			}
			setTimeout(function(){ 
					var fr = document.getElementById('adEmailFrame');
					var f = document.getElementById('adJoinEmail');
					var c = document.getElementById('adEmailContent');					
					if(fr && f && c){
						fr.src = join_email_url + "&email=" + encodeURIComponent(f.ad_email.value);
						c.style.display = "none";
						fr.parentNode.style.height = "225px";
						mboxUpdate(window.profileMboxName, 'clicked=thankYouForm');
						adEmailHideFrameClose();
					} 
				}, 
			1000);

	    }
	
		this.getActivationCode = function() {
			var e = document.getElementById('adErrDiv');		  		  
			var f = document.getElementById('adJoinEmail');
                        var popEmailTxt = document.getElementById("ad_email_field");

                        var btmForm = document.getElementById("joinEmail");
			var btmEmailTxt= (btmForm ? btmForm.getElementsByTagName("input")[0] : null);
			var btmEmailBtn = (btmForm ? btmForm.getElementsByTagName("input")[1] : null);
			mboxUpdate(window.profileMboxName, 'clicked=emailFormSubmit');
			
			if(e){ e.innerHTML = ""; }
			if(e && f && f.ad_email && this.validateEmail(f.ad_email.value)){
				if (btmEmailTxt && btmEmailBtn && popEmailTxt) {
				    btmEmailTxt.value = popEmailTxt.value;				
				    fireEvent(btmEmailBtn,"click");
				    adEmailClosePopup();
				    this.addScriptTag(service_url + "email-opt-in-service/" + window.serviceVersion + "/" + window.brand + "/" + encodeURIComponent(f.ad_email.value) + "?callBack=window.adEmailTester.setEmailCodeHandler" + "");
			  	}
		                tntLookForSubmit2();
			}else{
				e.innerHTML = "<p style='color:black !important;margin-left:-5px;'>*Required Information</p><p>Please enter an email address as &#34jdoe@email.com.&#34 The email address can be up to 75 characters long.</p>"
				var e2 = document.getElementById('adErrDiv2');
				if(e2){e2.style.color="#ab2826";}
			}
			return false;
		}
			
		this.validateEmail = function(_value) {		  		  
			var emailPattern = /^[a-zA-Z0-9._%+`!#$%^&*()-=:;\"\'\<\>?]+@(([A-Za-z0-9\-]+)\.)+([A-Za-z0-9]{2,4})$/;  
			if(!emailPattern.test(_value) || _value.length > 75){
				return false;
			}else{
				return true;
			}			
		}

		this.lookup = function() {
			var p = adEmailCheckTestParam();
			var campaign_url = service_url + "campaign-service/" + window.serviceVersion + "/" + window.brand + "/" + "EmailSuppress/"
			if(p == ''){
				temp = theCookies.split(";");
				for(var x=0;x<temp.length;x++){
					if(temp[x].indexOf("CoreID6")> -1){
						var coreID6 = temp[x].split("=");
						var coreID6cookie = coreID6[1];
						if(coreID6cookie != ""){	
							this.addScriptTag(campaign_url + coreID6cookie + "?callBack=window.adEmailTester.lookupHandler");
							return true;
						}
					}
				}
				//We want to do the popup if there is no core cookie - new user
				if(theCookies.indexOf("CoreID6") == -1){
					this.addScriptTag(campaign_url + "12345" + "?callBack=window.adEmailTester.lookupHandler");
					return true;
				}
				window.adDisplayDefault();
			}else{
				this.addScriptTag(campaign_url + p + "?callBack=window.adEmailTester.lookupHandler");
			}
		}
	} )// adEmailTester

