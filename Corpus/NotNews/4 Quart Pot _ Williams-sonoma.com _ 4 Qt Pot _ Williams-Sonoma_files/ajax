	window.adSmartMboxDefine = function(_mbox){
		if(typeof(mbox) != "undefined" && !document.getElementById(_mbox + '_div')){
		        var d = document.createElement('div');
		        d.id = _mbox + '_div';
		        document.body.insertBefore(d,document.body.firstChild);
		        mboxDefine(_mbox + '_div', _mbox).setFetcher(new mboxAjaxFetcher());
		}
	}
	window.adAddEmailTracking = function()
	{
		var f = document.getElementById('joinEmail');
		if(f){
			var a = f.getElementsByTagName('input');
			for(var i=0;i<a.length;i++){
				if(a[i].className && a[i].className == "btn_sign-up"){
					a[i].onmousedown = function(){ adSmartMboxDefine('mboxClickTrack');mboxUpdate('mboxClickTrack', 'clicked=footerSubmit'); }
					a[i].setAttribute('addig', 'handled');
				}
			}
		}else{
			setTimeout("window.adAddEmailTracking()", 100);
		}
	}
	
	mboxFactoryDefault.addOnLoad( function(){
		if(!adEmailCheckSessionCookie() || window.location.href.indexOf("&forcePop") > -1){
			window.adEmailTester.lookup();
		}
		window.adAddEmailTracking();
		if(typeof(dojo) != "undefined"){
			dojo.publish("/closeDialogs");


		}
	})


